				<string id="OneZeroOne.event.1002.message" value="%1%r&#xA;%2"></string>
				<string id="OneZeroOne.event.1001.message" value="%1%r&#xA;%2"></string>
				<string id="OneZeroOne.event.1000.message" value="%1%r&#xA;%2"></string>
